{
    "$schema": "https://docs.tolgee.io/cli-schema.json",
    "projectId": 19983,
    "format": "JSON_TOLGEE",
    "patterns": [
        "./src/**/*.ts?(x)"
    ],
    "push": {
        "filesTemplate": "./i18n/{{language}}.json",
