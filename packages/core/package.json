{
    "name": "@openmarch/core",
    "type": "module",
    "description": "Core utilities and types for OpenMarch applications",
    "license": "AGPL-3.0",
    "version": "1.1.0",
    "homepage": "https://openmarch.com",
    "author": "OpenMarch LLC <contact@openmarch.com>",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenMarch/OpenMarch.git"
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup --format esm,cjs",
        "pretest": "pnpm build",
        "test": "vitest run --silent",
        "test:watch": "vitest"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "devDependencies": {
        "@openmarch/config": "workspace:*",
        "fast-check": "^4.3.0",
        "tsup": "^8.5.0",
        "typescript": "latest",
        "vitest": "^2.1.9"
    },
    "dependencies": {
        "svg-path-commander": "^2.1.11",
        "uuid": "^11.1.0"
    }
}
