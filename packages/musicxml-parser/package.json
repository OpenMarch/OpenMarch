{
    "name": "@openmarch/musicxml-parser",
    "license": "AGPL-3.0",
    "type": "module",
    "version": "1.0.0",
    "types": "dist/index.d.ts",
    "main": "dist/index.cjs",
    "publishConfig": {
        "@openmarch:registry": "https://npm.pkg.github.com"
    },
    "scripts": {
        "build": "tsup --format esm,cjs",
        "dev": "tsup --watch",
        "test": "vitest run --silent",
        "test:watch": "vitest"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "peerDependencies": {
        "typescript": "^5"
    },
    "devDependencies": {
        "vitest": "^2.1.9",
        "tsup": "^8.5.0"
    }
}
